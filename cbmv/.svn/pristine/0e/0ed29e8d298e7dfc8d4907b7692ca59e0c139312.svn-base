# 端口
server.port=8080
# 项目名
server.servlet.context-path=/cbmv

#数据源
#加载驱动
#spring.datasource.driver-class-name=com.mysql.jdbc.Driver
#链接
spring.datasource.url=jdbc:mysql://localhost:3306/cbmv?useSSL=false&serverTimezone=UTC
#用户名
spring.datasource.username=root
#密码没有可以不写

#最大活动数
spring.datasource.druid.max-active=50
#最小空闲数
spring.datasource.druid.min-idle=10
#初始化连接数
spring.datasource.druid.initial-size=10


#配置监控
#ip白名单
sys.druid.stat.allow=172.0.0.1
#登录看信息的账户
sys.druid.stat.loginUsername=admin
#登录看信息的账户密码
sys.druid.stat.loginPassword=123456




sys_page_size=5
sys_charset=utf-8



